{
  "manifest_version": 3,
  "name": "Agent",
  "version": "50.1.0.2",
  "description": "Agent",
  "permissions": [
    "activeTab",
    "storage",
    "unlimitedStorage",
    "scripting",
    "tabs",
    "tabGroups",
    "debugger",
    "sidePanel",
    "bookmarks",
    "history",
    "webNavigation",
    "downloads",
    "audioCapture"
  ],
  "host_permissions": [
    "<all_urls>"
  ],
  "content_security_policy": {
    "extension_pages": "script-src 'self'; frame-src https://www.youtube.com https://www.youtube-nocookie.com; media-src 'self' blob: data: mediastream: https://pub-80f8a01e6e8b4239ae53a7652ef85877.r2.dev https://cdn.browseros.com; object-src 'self'; connect-src *"
  },
  "background": {
    "service_worker": "background.js"
  },
  "action": {
    "default_icon": {
      "16": "assets/icon16.png",
      "48": "assets/icon48.png",
      "128": "assets/icon128.png"
    }
  },
  "chrome_url_overrides": {
    "newtab": "newtab.html"
  },
  "side_panel": {
    "default_path": "sidepanel.html"
  },
  "commands": {
    "toggle-panel": {
      "suggested_key": {
        "default": "Ctrl+E",
        "mac": "Command+E"
      },
      "description": "Toggle the Nxtscape side panel"
    }
  },
  "icons": {
    "16": "assets/icon16.png",
    "48": "assets/icon48.png",
    "128": "assets/icon128.png"
  },
  "options_ui": {
    "page": "browseros-settings.html",
    "open_in_tab": true
  }
}